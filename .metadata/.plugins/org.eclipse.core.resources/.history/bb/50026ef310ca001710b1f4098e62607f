package by.bntu.fitr.laba7;

import java.util.Random;

public class Creator {
	
	public int[][] create(){
		Random rand = new Random();
		int a = rand.nextInt(10);
		int b = rand.nextInt(10);
		int[][] matrix = new int[a][b];
		for(int i = 0; i<a; i++){
			for(int j = 0; j<b; j++){
				matrix[i][j] = rand.nextInt(15);
			}
		}
		return matrix;
	}

}
