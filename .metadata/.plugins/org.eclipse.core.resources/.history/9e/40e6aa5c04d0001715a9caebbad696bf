package by.bntu.fitr.xcode;

import java.util.Arrays;

public class Matrix {
private int[][] matrix;
	
	public int[][] getMatrix(){
		return matrix;
	}
	
	public int giveElm(int i, int j){
		return matrix[i][j];
	}
	
	public void getElm(int i, int j, int num){
		matrix[i][j] = num;
	}
	
	public Matrix(int a, int b){
		matrix = new  int[a][b];
	}
	
	public int getLength(){
		return matrix.length;
	}
}
